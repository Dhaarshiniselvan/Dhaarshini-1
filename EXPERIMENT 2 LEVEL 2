#include<stdio.h>
void main()
{
    char grade;
    printf("enter a grade (E,V,G,A,F):");
    scanf("%c",&grade);
    switch(grade)
    {
        case 'E':
        printf("Excellent\n");
        break;
        
        case 'V':
        printf("Very good\n");
        break;
        
        case 'G':
        printf("Good\n");
        break;
        
        case 'A':
        printf("Average\n");
        break;
        
        case 'F':
        printf("Fail\n");
        break;
        
        Default:
        printf("invalide grade .\n");
    }

}

