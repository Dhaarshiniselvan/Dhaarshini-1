#include<stdio.h>
void main()
{
    int n,i,evencount=0,oddcount=0;
    printf("enter the number of element in array:");
    scanf("%d",&n);
    int arr [n];
    printf("enter %d element in array :\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr [i]);
        if(arr[i]%2==0)
        {
            evencount ++;
        }
        else
        {
            oddcount ++;
        }
    }
    printf("total even element :%d\n",evencount);
    printf("total odd element :%d\n",oddcount);
    
    
}


