#include<stdio.h>
void main()
{
    char ch;
    printf("enter an alphabet:");
    scanf("%c",&ch);
    if(ch==a,ch==e,ch==i,ch==o,ch==u,ch==A,ch==E,ch==I,ch==O,ch==U);
    {
        printf("%cis vowel.\n",ch);
        
    }
    else if((ch>=a&&ch<=z)||(ch>=A&&ch<=z))
    {
        printf("%c is a consonant.\n",ch);
    }
    else
    {
        printf("Invalid input,not an alphabet.\n");
    }
}
